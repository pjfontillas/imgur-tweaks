{
    "manifest_version": 2,
    "name": "Imgur Tweaks",
    "description": "This Google Chrome extension automatically extends functionality on the Imgur site.",
    "version": "1.1.2",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["http://imgur.com/*"],
            "js": ["jquery.min.js", "imgur-tweaks.js"],
            "css": ["imgur-tweaks.css"]
        }
    ],
    "web_accessible_resources": ["jquery-1.10.2.min.map"]
}